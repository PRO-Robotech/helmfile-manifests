                          - name
                        mode: tag
                        valueUri: /api/clusters/{clusterName}/k8s/apis/provider.sgroups.io/v1alpha1/namespaces/{namespace}/addressgroups
                      type: listInput
                    namespace:
                      customProps:
                        keysToLabel:
                          - status
                          - addressGroupName
                        keysToValue:
                          - metadata
                          - name
                        mode: tag
                        valueUri: /api/clusters/{clusterName}/k8s/api/v1/namespaces
                      type: listInput
                  required:
                    - apiVersion
                    - kind
                    - name
                  type: object
                serviceRef:
                  description: ServiceRef is a reference to the Service resource
                  properties:
                    apiVersion:
                      description: APIVersion of the referenced object
                      enum:
                        - netguard.sgroups.io/v1alpha1
                      type: string
                    kind:
                      description: Kind of the referenced object
                      enum:
                        - Service
                      type: string
                    name:
                      customProps:
                        keysToLabel:
                          - status
                          - addressGroupName
                        keysToValue:
                          - metadata
                          - name
                        mode: tag
                        valueUri: "/api/clusters/{clusterName}/k8s/apis/netguard.sgroups.io/v1alpha1/namespaces/{relatedFieldValue}/servicealiases"
                        relatedValuePath:
                        - spec
                        - serviceRef
                        - namespace
                      type: listInput
                    namespace:
                      customProps:
                        keysToLabel:
                          - status
                          - addressGroupName
                        keysToValue:
                          - metadata
                          - name
                        mode: tag
                        valueUri: /apis/in-cloud.io/v1alpha1/instances
                      type: listInput
                  required:
