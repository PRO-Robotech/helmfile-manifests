            - kind
          - - spec
            - networkRef
            - apiVersion
          - - spec
            - networkRef
            - kind
        customizationId: default-/provider.sgroups.io/v1alpha1/networkbindings
        schema:
          properties:
            spec:
              properties:
                addressGroupRef:
                  properties:
                    name:
                      customProps:
                        keysToLabel:
                          - status
                          - addressGroupName
                        keysToValue:
                          - metadata
                          - name
                        mode: tag
                        valueUri: /api/clusters/{clusterName}/k8s/apis/provider.sgroups.io/v1alpha1/namespaces/{namespace}/addressgroups
                      type: listInput
                networkRef:
                  properties:
                    name:
                      customProps:
                        criteria:
                          keysToValue:
                            - status
                            - isBound
                          type: equals
                          value: false
                        keysToLabel:
                          - spec
                          - cidr
                        keysToValue:
                          - metadata
                          - name
                        mode: tag
                        valueUri: /api/clusters/{clusterName}/k8s/apis/provider.sgroups.io/v1alpha1/namespaces/{namespace}/networks
                      type: listInput
        strategy: merge

    CFONetworks: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsOverride
      metadata:
        name: provider.sgroups.io.v1alpha1.networks
      spec:
        hidden:
