            type: string
          - jsonPath: .spec.cidr
            name: CIDR
            type: string
          - jsonPath: .status.addressGroupRef.name
            name: AddressGroup
            type: string
          - jsonPath: .status.bindingRef.name
            name: AddressGroupBinding
            type: string
