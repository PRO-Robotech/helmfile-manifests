    CFPAddressGroups: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsPrefill
      metadata:
        name: provider.sgroups.io.v1alpha1.addressgroups
      spec:
        customizationId: default-/provider.sgroups.io/v1alpha1/addressgroups
        values:
          - path:
              - metadata
              - name
            value: example
          - path:
              - spec
              - defaultAction
            value: DROP
          - path:
              - spec
              - logs
            value: true
          - path:
              - spec
              - trace
            value: true

    CFPieagagrules: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsPrefill
      metadata:
        name: provider.sgroups.io.v1alpha1.ieagagrules
      spec:
        customizationId: default-/provider.sgroups.io/v1alpha1/ieagagrules
        values:
          - path:
              - metadata
              - generateName
            value: dynamic-
          - path:
              - spec
              - action
            value: ACCEPT
          - path:
              - spec
              - addressGroup
              - apiVersion
            value: provider.sgroups.io/v1alpha1
          - path:
              - spec
              - addressGroup
              - kind
            value: AddressGroup
          - path:
              - spec
              - addressGroupLocal
              - apiVersion
            value: provider.sgroups.io/v1alpha1
          - path:
              - spec
              - addressGroupLocal
              - kind
            value: AddressGroup
          - path:
              - spec
              - cidr
            value: ""
          - path:
              - spec
              - traffic
            value: INGRESS
          - path:
              - spec
              - transport
            value: TCP

    CFPiecidragrules: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsPrefill
