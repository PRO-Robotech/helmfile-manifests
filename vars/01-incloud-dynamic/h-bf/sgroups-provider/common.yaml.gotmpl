            value: ""
          - path:
              - spec
              - traffic
            value: INGRESS
          - path:
              - spec
              - transport
            value: TCP

    CFPNetworkBindings: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsPrefill
      metadata:
        name: provider.sgroups.io.v1alpha1.networkbindings
      spec:
        customizationId: default-/provider.sgroups.io/v1alpha1/networkbindings
        values:
          - path:
              - metadata
              - generateName
            value: network-binding-
          - path:
              - spec
              - addressGroupRef
              - apiVersion
            value: provider.sgroups.io/v1alpha1
          - path:
              - spec
              - addressGroupRef
              - kind
            value: AddressGroup
          - path:
              - spec
              - networkRef
              - apiVersion
            value: provider.sgroups.io/v1alpha1
          - path:
              - spec
              - networkRef
              - kind
            value: Network

    CFPNetworks: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsPrefill
      metadata:
        name: provider.sgroups.io.v1alpha1.networks
      spec:
        customizationId: default-/provider.sgroups.io/v1alpha1/networks
        values:
          - path:
              - metadata
              - generateName
            value: dynamic-
          - path:
              - spec
              - cidr
            value: 1.1.1.1/32

    CFOAddressGroups: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsOverride
      metadata:
        name: provider.sgroups.io.v1alpha1.addressgroups
      spec:
        hidden:
          - - apiVersion
          - - kind
          - - status
          - - networks
          - - metadata
            - creationTimestamp
          - - metadata
            - deletionGracePeriodSeconds
          - - metadata
            - deletionTimestamp
          - - metadata
            - finalizers
          - - metadata
            - generateName
          - - metadata
            - generation
          - - metadata
            - managedFields
          - - metadata
            - labels
          - - metadata
            - annotations
          - - metadata
            - ownerReferences
          - - metadata
            - resourceVersion
          - - metadata
            - selfLink
          - - metadata
            - uid
        customizationId: default-/provider.sgroups.io/v1alpha1/addressgroups
        schema: {}
        strategy: merge

    CFOieagagrules: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsOverride
      metadata:
        name: provider.sgroups.io.v1alpha1.ieagagrules
      spec:
        hidden:
          - - apiVersion
          - - kind
          - - status
          - - networks
          - - metadata
          - - spec
            - logs
          - - spec
            - trace
          - - spec
            - priority
          - - spec
            - addressGroup
            - apiVersion
          - - spec
            - addressGroup
            - kind
          - - spec
            - addressGroupLocal
            - apiVersion
          - - spec
            - addressGroupLocal
            - kind
          - - spec
            - addressGroupLocal
            - namespace
        customizationId: default-/provider.sgroups.io/v1alpha1/ieagagrules
        schema:
          properties:
            spec:
              description: IEAgAgRuleSpec defines the desired state of IEAgAgRule.
              type: object
              required:
              - action
              - addressGroup
              - addressGroupLocal
              - ports
              - traffic
              - transport
              properties:
                action:
                  description: Rule action
                  type: string
                  enum:
                  - ACCEPT
                  - DROP
                addressGroup:
                  description: Reference to the address group
                  type: object
                  required:
                  - apiVersion
                  - kind
                  - name
                  properties:
                    apiVersion:
                      description: APIVersion of the referenced object
                      type: string
                    kind:
                      description: Kind of the referenced object
                      type: string
                    name:
                      description: Name of the referenced object
                      type: listInput
                      customProps:
                        keysToLabel:
                        - metadata
                        - name
                        keysToValue:
                        - metadata
                        - name
                        mode: tag
                        valueUri: "/api/clusters/{clusterName}/k8s/apis/provider.sgroups.io/v1alpha1/namespaces/{namespace}/addressgroups"
                    namespace:
                      description: Namespace of the referenced object
                      type: listInput
                      customProps:
                        keysToLabel:
                        - metadata
                        - name
                        keysToValue:
                        - metadata
                        - name
                        mode: tag
                        valueUri: "/api/clusters/{clusterName}/k8s/api/v1/namespaces"
                addressGroupLocal:
                  description: Reference to the local address group
                  type: object
                  required:
                  - apiVersion
                  - kind
                  - name
                  properties:
                    apiVersion:
                      description: APIVersion of the referenced object
                      type: string
                    kind:
                      description: Kind of the referenced object
                      type: string
                    name:
                      description: Name of the referenced object
                      type: listInput
                      customProps:
                        keysToLabel:
                        - metadata
                        - name
                        keysToValue:
                        - metadata
                        - name
                        mode: tag
                        valueUri: "/api/clusters/{clusterName}/k8s/apis/provider.sgroups.io/v1alpha1/namespaces/{namespace}/addressgroups"
                    namespace:
                      description: Namespace of the referenced object
                      type: string
                logs:
                  description: Whether to enable logs
                  type: boolean
                ports:
                  description: List of source and destination ports
                  type: array
                  items:
                    description: AccPorts defines source and destination ports
                    type: object
                    required:
                    - d
                    properties:
                      d:
                        description: Destination port or port range
                        type: string
                priority:
                  description: Rule priority
                  type: object
                  properties:
                    value:
                      description: Value of the priority
                      type: integer
                      format: int32
                trace:
                  description: Whether to enable trace
                  type: boolean
                traffic:
                  description: Direction of traffic flow
                  type: string
                  enum:
                  - INGRESS
                  - EGRESS
                transport:
                  description: Transport protocol for the rule
                  type: string
                  enum:
                  - TCP
                  - UDP
            
        strategy: replace

    CFOiecidragrules: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsOverride
      metadata:
        name: provider.sgroups.io.v1alpha1.iecidragrules
      spec:
        hidden:
