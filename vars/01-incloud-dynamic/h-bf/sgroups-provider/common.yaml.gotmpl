            - addressGroup
            - apiVersion
          - - spec
            - addressGroup
            - kind
          - - spec
            - addressGroup
            - namespace
        customizationId: default-/provider.sgroups.io/v1alpha1/iecidragrules
        schema:
          properties:
            spec:
              description: IECidrAgRuleSpec defines the desired state of IECidrAgRule.
              type: object
              required:
              - action
              - addressGroup
              - cidr
              - ports
              - traffic
              - transport
              properties:
                action:
                  description: RuleAction represents allowed actions for SgSgIcmpRule
                  type: string
                  enum:
                  - ACCEPT
                  - DROP
                addressGroup:
                  description: Reference to the address group
                  type: object
                  required:
                  - apiVersion
                  - kind
                  - name
                  properties:
                    apiVersion:
                      description: APIVersion of the referenced object
                      type: string
                    kind:
                      description: Kind of the referenced object
                      type: string
                    name:
                      description: Name of the referenced object
                      type: listInput
                      customProps:
                        keysToLabel:
                        - status
                        - addressGroupName
                        keysToValue:
                        - metadata
                        - name
                        mode: tag
                        valueUri: "/api/clusters/{clusterName}/k8s/apis/provider.sgroups.io/v1alpha1/namespaces/{namespace}/addressgroups"
                    namespace:
                      description: Namespace of the referenced object
                      type: string
                cidr:
                  description: CIDR notation for IP range, e.g., "192.168.0.0/16"
                  type: string
                logs:
                  description: Whether to enable logs
                  type: boolean
                ports:
                  description: List of source and destination ports
                  type: array
                  items:
                    description: AccPorts defines source and destination ports
                    type: object
                    required:
                    - d
                    properties:
                      d:
                        description: Destination port or port range
                        type: string
                priority:
                  description: Rule priority
                  type: object
                  properties:
                    value:
                      description: Value of the priority
                      type: integer
                      format: int32
                trace:
                  description: Whether to enable trace
                  type: boolean
                traffic:
                  description: Direction of traffic flow
                  type: string
                  enum:
                  - INGRESS
                  - EGRESS
                transport:
                  description: Transport protocol for the rule
                  type: string
                  enum:
                  - TCP
                  - UDP

        strategy: replace

    CFONetworkBindings: |
      ---
      apiVersion: front.in-cloud.io/v1alpha1
      kind: CustomFormsOverride
      metadata:
        name: provider.sgroups.io.v1alpha1.networkbindings
      spec:
        hidden:
          - - apiVersion
          - - kind
          - - status
          - - networks
          - - metadata
          - - spec
